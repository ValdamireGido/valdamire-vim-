" MSBuild settings 
set makeprg=build.bat;
set errorformat+=\ %#%f(%l\\\,%c):\ %m
set errorformat+=%-G%.%#
set shell=cmd
set shellcmdflag =/c

call plug#begin('~/AppData/Local/nvim/plugged')
Plug '/c/ProgramData/chocolatey/bin/fzf'
Plug 'junegunn/fzf', { 'dir': '~/.fzf', 'do': './install --all' }
call plug#end()
